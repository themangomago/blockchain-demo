.well
  form.form-horizontal
    fieldset
      legend Wallet  
      .form-group
        label.col-sm-2.control-label() Seed:
        .col-sm-10
          input.form-control(id='aliceSeed', type='text', value='Super Secret Seed', onkeyup='calcAliceCredentials();' disabled)
      .form-group
        label.col-sm-2.control-label() Private Key:
        .col-sm-10
          input.form-control(id='alicePrivKey', type='text', value='', onkeyup='' disabled)
      .form-group
        label.col-sm-2.control-label() Public Key:
        .col-sm-10
          input.form-control(id='alicePubKey', type='text', value='', onkeyup='' disabled)
      .form-group
        label.col-sm-2.control-label() Public Key Hash (PKH):
        .col-sm-10
          input.form-control(id='alicePKH', type='text', value='', onkeyup='' disabled)
      hr
      .form-group
        label.col-sm-2.control-label() Balance:
        .col-sm-10
          input.form-control(id='aliceBalance', type='text', value='0.00', onkeyup='')